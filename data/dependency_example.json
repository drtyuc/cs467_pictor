{
	"commands": [
		{
			"tuple": "look at table2",
			"dependencies": [{
				"method": "isObjectVisible()",
				"parameter": "table2",
				"expectedvalue": true,
				"text": "Cannot see table2"
			}, {
				"method": "isRoomLighted()",
				"parameter": "",
				"expectedvalue": true,
				"text": "Room is dark, cannot see table2"
			}, {
				"method": "isObjectInRoom()",
				"parameter": "table2",
				"expectedvalue": true,
				"text": "table2 is not in this room"
			}],
			"actions": [{
				"method": "setObjectVisible()",
				"parameter": "apple1",
				"text": "apple1 is on table2"
			}, {
				"method": "setObjectVisible()",
				"parameter": "knife1",
				"text": "knife1 is on table2"
			}, {
				"method": "setObjectVisible()",
				"parameter": "matches1",
				"text": "matches1 is on table2"
			}]
		}, {
			"tuple": "equip helmet1",
			"dependencies": [{
				"method": "isObjectInInventory()",
				"parameter": "helmet1",
				"expectedvalue": true,
				"text": "helmet1 is not in your inventory"
			}, {
				"method": "isObjectEquipped()",
				"parameter": "helmet1",
				"expectedvalue": false,
				"text": "helmet1 is already equipped"
			}],
			"actions": [{
				"method": "setObjectEquipped()",
				"parameter": "helmet1",
				"text": "helmet1 is now equipped"
			}]
		}, {
			"tuple": "wield axe1",
			"dependencies": [{
				"method": "isObjectInInventory()",
				"parameter": "axe1",
				"expectedvalue": true,
				"text": "axe1 is not in your inventory"
			}, {
				"method": "isGhostInRoom()",
				"parameter": "",
				"expectedvalue": true,
				"text": "No ghosts are in the room"
			}, {
				"method": "isGhostVisible()",
				"parameter": "",
				"expectedvalue": true,
				"text": "Cannot see any ghosts"
			}],
			"actions": [{
				"method": "setGhostHealth()",
				"parameter": "axe1",
				"text": ""
			}]
		}, {
			"tuple": "drink bottle1",
			"dependencies": [{
				"method": "isObjectInInventory()",
				"parameter": "bottle1",
				"expectedvalue": true,
				"text": "bottle1 is not in your inventory"
			}],
			"actions": [{
				"method": "removeInventoryObject()",
				"parameter": "bottle1",
				"text": "bottle1 is consumed"
			}, {
				"method": "setPlayerHealth()",
				"parameter": "bottle1",
				"text": "player's health has increased"
			}]
		}
	]
}
